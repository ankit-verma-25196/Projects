<?xml version="1.0" encoding="utf-8"?>

<TabHost xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="in.thekites.quizlite.ResultActivity"
    android:id="@+id/tabhost">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/da">

        <TabWidget
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@android:id/tabs"
            android:background="#B0E0E6"
            >

        </TabWidget>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@android:id/tabcontent"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/cplusplus"
                android:orientation="vertical"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/res2"
                    android:layout_alignParentTop="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="50dp"
                    android:id="@+id/img1"/>

                <TextView
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text1"
                    android:layout_below="@id/img1"
                    android:text="Points:"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    />

                <TextView
                    android:textColor="#00008B"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text2"
                    android:layout_marginTop="30dp"
                    android:layout_below="@id/img1"
                    android:layout_toRightOf="@id/text1"/>

                <TextView
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text3"
                    android:layout_below="@id/text1"
                    android:layout_marginTop="20dp"
                    android:text="Time Taken:"
                    android:textSize="25dp"
                    android:typeface="serif"
                    />

                <TextView
                    android:textColor="#00008b"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text4"
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/text1"
                    android:layout_toRightOf="@id/text3"/>

                <TextView
                    android:textStyle="bold"
                    android:textColor="#000000"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text5"
                    android:text="Time Left:"
                    android:layout_below="@id/text3"
                    android:layout_marginTop="20dp"
                    android:textSize="25dp"
                    android:typeface="serif"
                    />

                <TextView
                    android:textColor="#00008b"
                    android:textSize="25dp"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/text6"
                    android:layout_toRightOf="@id/text5"
                    android:layout_below="@id/text3"/>

                <RelativeLayout
                    android:id="@+id/downbar"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:background="#cc2F4F4F"
                    android:layout_alignParentBottom="true">

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentLeft="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_restart"
                        android:background="@drawable/ret"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_home"
                        android:background="@drawable/homy"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentRight="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_close"
                        android:background="@drawable/close"/>


                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/java"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/res2"
                    android:layout_marginLeft="50dp"
                    android:id="@+id/im1"/>

                <TextView
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt1"
                    android:layout_below="@id/im1"
                    android:text="Points:"
                    android:textSize="25dp"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:typeface="serif"
                    />

                <TextView
                    android:textColor="#00008b"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:layout_marginTop="30dp"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt2"
                    android:layout_below="@id/im1"
                    android:layout_toRightOf="@id/txt1"/>

                <TextView
                    android:textStyle="bold"
                    android:textColor="#000000"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt3"
                    android:layout_below="@id/txt1"
                    android:layout_marginTop="20dp"
                    android:text="Time Taken:"
                    android:textSize="25dp"
                    android:typeface="serif"
                    />

                <TextView
                    android:textColor="#00008b"
                    android:textStyle="bold"
                    android:layout_marginTop="30dp"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt4"
                    android:layout_below="@id/txt1"
                    android:layout_toRightOf="@id/txt3"/>

                <TextView
                    android:textStyle="bold"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt5"
                    android:text="Time Left:"
                    android:layout_below="@id/txt3"
                    android:layout_marginTop="20dp"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:textColor="#000000"
                    />

                <TextView
                    android:textStyle="bold"
                    android:textColor="#00008b"
                    android:layout_marginTop="30dp"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txt6"
                    android:layout_toRightOf="@id/txt5"
                    android:layout_below="@id/txt3"/>

                <RelativeLayout
                    android:id="@+id/downbarj"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:background="#cc2F4F4F"
                    android:layout_alignParentBottom="true">

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentLeft="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_restartj"
                        android:background="@drawable/ret"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_homej"
                        android:background="@drawable/homy"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentRight="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_closej"
                        android:background="@drawable/close"/>


                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/android"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/res2"
                    android:layout_marginLeft="50dp"
                    android:id="@+id/i1"/>

                <TextView
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx1"
                    android:layout_below="@id/i1"
                    android:text="Points:"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:typeface="serif"
                    />

                <TextView
                    android:textSize="25dp"
                    android:textColor="#00008b"
                    android:textStyle="bold"
                    android:typeface="serif"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx2"
                    android:layout_below="@id/i1"
                    android:layout_marginTop="30dp"
                    android:layout_toRightOf="@id/tx1"/>

                <TextView
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx3"
                    android:layout_below="@id/tx1"
                    android:layout_marginTop="20dp"
                    android:text="Time Taken:"
                    android:textSize="25dp"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:typeface="serif"
                    />

                <TextView
                    android:textColor="#00008b"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:typeface="serif"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx4"
                    android:layout_below="@id/tx1"
                    android:layout_toRightOf="@id/tx3"/>

                <TextView
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx5"
                    android:text="Time Left:"
                    android:layout_below="@id/tx3"
                    android:layout_marginTop="20dp"
                    android:textSize="25dp"
                    android:textColor="#000000"
                    android:textStyle="bold"
                    android:typeface="serif"
                    />

                <TextView
                    android:textSize="25dp"
                    android:textStyle="bold"
                    android:textColor="#00008b"
                    android:typeface="serif"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tx6"
                    android:layout_toRightOf="@id/tx5"
                    android:layout_below="@id/tx3"/>

                <RelativeLayout
                    android:id="@+id/downbara"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:background="#cc2F4F4F"
                    android:layout_alignParentBottom="true">

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentLeft="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_restarta"
                        android:background="@drawable/ret"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_homea"
                        android:background="@drawable/homy"/>

                    <ImageButton
                        android:layout_centerInParent="true"
                        android:layout_alignParentRight="true"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:id="@+id/button_closea"
                        android:background="@drawable/close"/>


                </RelativeLayout>

            </RelativeLayout>


        </FrameLayout>

    </LinearLayout>



</TabHost>
